#include <stdio.h>

int main(){
    int n = 19;
    for(int i=1 ; i<=n ; i++){
        for(int j=1 ; j<=39 ; j++){
            if (){
                printf("*");
            } else {
                printf(" ");
            }
        }
        printf("\n");
    }

    return 0;
}
