#include <stdio.h>

int main() {
    int n = 10;

    for(int i=1 ; i<=n ; i++){
         for(int j=1 ; j<=n ; j++){
            if(i==2 && j<=6 || i==n && j<10 || j==1 && i>1 || j==9 && i>4 || j==5 && i>3 && i<9 || i==5 && j==4 || i==2 && j>7 || j==9 && i<4){ 
                printf("* ");
            } else {
                printf("  ");
            }
        }
        printf("\n");
    }
    
    return 0;
}
